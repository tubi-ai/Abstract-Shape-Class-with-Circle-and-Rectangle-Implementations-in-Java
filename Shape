package com.company;

abstract class Shape {
    public double x;
    public double y;
    abstract double calculateArea();
    abstract double calculateCircumference();
    public double getX(){
        return x;
    }
    public double getY(){
        return y;
    }
    public void setX(double x) {
        this.x = x;
    }
    public void setY(double y) {
        this.y = y;
    }
}
